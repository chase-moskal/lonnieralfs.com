{
	"private": true,
	"name": "lonnieralfs.com",
	"version": "0.1.0-dev.0",
	"description": "website for lonnie ralfs",
	"license": "ISC",
	"scripts": {
		"prepare": "run-s clean && run-p importmap && run-s test && echo âœ” done build",
		"clean": "rimraf dist && mkdirp dist",
		"importmap": "importly < importly.config > dist/importmap.json",
		"images": "run-p images1 images2 images3",
		"images1": "jpegger --indir images --outdir dist/images/large --quality 90",
		"images2": "jpegger --indir images --outdir dist/images/medium --size 2000 --quality 80",
		"images3": "jpegger --indir images --outdir dist/images/small --size 1200 --quality 80",
		"test": "exit 0",
		"start": "serve -n"
	},
	"devDependencies": {
		"importly": "0.0.0-dev.4",
		"mkdirp": "^0.5.1",
		"npm-run-all": "^4.1.5",
		"request": "^2.88.0",
		"rimraf": "^2.6.3",
		"serve": "^11.0.0"
	},
	"dependencies": {
		"jpegger": "0.1.0-dev.0"
	}
}
